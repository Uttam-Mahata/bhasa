// হ্যাশ ম্যাপ (Hash Maps)

ধরি ব্যক্তি = {
    "নাম": "রহিম",
    "বয়স": ২৫,
    "শহর": "ঢাকা"
};

লেখ("ব্যক্তির তথ্য:");
লেখ(ব্যক্তি);

লেখ("নাম:");
লেখ(ব্যক্তি["নাম"]);

লেখ("বয়স:");
লেখ(ব্যক্তি["বয়স"]);

লেখ("শহর:");
লেখ(ব্যক্তি["শহর"]);

// সংখ্যা দিয়ে হ্যাশ ম্যাপ
ধরি স্কোর = {
    ১: "প্রথম",
    ২: "দ্বিতীয়",
    ৩: "তৃতীয়"
};

লেখ("স্কোর:");
লেখ(স্কোর[১]);
লেখ(স্কোর[২]);
লেখ(স্কোর[৩]);

