// ===========================================
// 9. Type Conversion Chain
// ===========================================
লেখ("9. Type Conversion Chain");
লেখ("------------------------");
ধরি start_val = 200;
লেখ("Starting with: 200");
লেখ("");

ধরি chain_byte = বাইট(start_val);
লেখ("As Byte: ");
লেখ(chain_byte);
লেখ("Type: " + টাইপ(chain_byte));

ধরি chain_short = ছোট_সংখ্যা(chain_byte);
লেখ("Byte to Short: ");
লেখ(chain_short);
লেখ("Type: " + টাইপ(chain_short));

ধরি chain_int = পূর্ণসংখ্যা(chain_short);
লেখ("Short to Int: ");
লেখ(chain_int);
লেখ("Type: " + টাইপ(chain_int));

ধরি chain_long = দীর্ঘ_সংখ্যা(chain_int);
লেখ("Int to Long: ");
লেখ(chain_long);
লেখ("Type: " + টাইপ(chain_long));

ধরি chain_float = দশমিক(chain_long);
লেখ("Long to Float: ");
লেখ(chain_float);
লেখ("Type: " + টাইপ(chain_float));

ধরি chain_double = দশমিক_দ্বিগুণ(chain_float);
লেখ("Float to Double: ");
লেখ(chain_double);
লেখ("Type: " + টাইপ(chain_double));
লেখ("");

