// ===========================================
// 1. Basic Type Casting
// ===========================================
লেখ("1. Basic Type Casting");
লেখ("---------------------");
ধরি original = 100;
লেখ("Original value: 100");
লেখ("");

// Cast to Byte (0-255)
ধরি as_byte = বাইট(original);
লেখ("বাইট (Byte): ");
লেখ(as_byte);
লেখ("Type: " + টাইপ(as_byte));
লেখ("");

// Cast to Short (-32768 to 32767)
ধরি as_short = ছোট_সংখ্যা(original);
লেখ("ছোট_সংখ্যা (Short): ");
লেখ(as_short);
লেখ("Type: " + টাইপ(as_short));
লেখ("");

// Cast to Int
ধরি as_int = পূর্ণসংখ্যা(original);
লেখ("পূর্ণসংখ্যা (Int): ");
লেখ(as_int);
লেখ("Type: " + টাইপ(as_int));
লেখ("");

// Cast to Long
ধরি as_long = দীর্ঘ_সংখ্যা(original);
লেখ("দীর্ঘ_সংখ্যা (Long): ");
লেখ(as_long);
লেখ("Type: " + টাইপ(as_long));
লেখ("");

// Cast to Float
ধরি as_float = দশমিক(original);
লেখ("দশমিক (Float): ");
লেখ(as_float);
লেখ("Type: " + টাইপ(as_float));
লেখ("");

// Cast to Double
ধরি as_double = দশমিক_দ্বিগুণ(original);
লেখ("দশমিক_দ্বিগুণ (Double): ");
লেখ(as_double);
লেখ("Type: " + টাইপ(as_double));
লেখ("");

