// জটিল অ্যালগরিদম পরীক্ষা - সম্পূর্ণ বাংলা কীওয়ার্ড
// Complex Algorithm Test with Full Bengali Keywords

লেখ("=== জটিল অ্যালগরিদম পরীক্ষা ===");
লেখ("");

// ১. দ্রুত সাজানো (Quick Sort) - সম্পূর্ণ বাংলায়
লেখ("১. দ্রুত সাজানো (Quick Sort):");

ফাংশন বিভাজন(তালিকা, নিম্ন, উচ্চ) {
    ধরি পিভট = তালিকা[উচ্চ];
    ধরি সূচক = নিম্ন - ১;

    পর্যন্ত(ধরি জে = নিম্ন; জে < উচ্চ; জে = জে + ১) {
        যদি (তালিকা[জে] < পিভট) {
            সূচক = সূচক + ১;
            ধরি অস্থায়ী = তালিকা[সূচক];
            তালিকা[সূচক] = তালিকা[জে];
            তালিকা[জে] = অস্থায়ী;
        }
    }

    ধরি অস্থায়ী = তালিকা[সূচক + ১];
    তালিকা[সূচক + ১] = তালিকা[উচ্চ];
    তালিকা[উচ্চ] = অস্থায়ী;

    ফেরত সূচক + ১;
}

ফাংশন দ্রুত_সাজানো(তালিকা, নিম্ন, উচ্চ) {
    যদি (নিম্ন < উচ্চ) {
        ধরি বিভাজন_সূচক = বিভাজন(তালিকা, নিম্ন, উচ্চ);
        দ্রুত_সাজানো(তালিকা, নিম্ন, বিভাজন_সূচক - ১);
        দ্রুত_সাজানো(তালিকা, বিভাজন_সূচক + ১, উচ্চ);
    }
    ফেরত তালিকা;
}

ধরি অসাজানো_তালিকা = [৬৪, ৩৪, ২৫, ১২, ২২, ১১, ৯০, ৮৮, ৪৫];
লেখ("সাজানোর আগে: " + লেখা(অসাজানো_তালিকা));

ধরি সাজানো_তালিকা = দ্রুত_সাজানো(অসাজানো_তালিকা, ০, দৈর্ঘ্য(অসাজানো_তালিকা) - ১);
লেখ("সাজানোর পরে: " + লেখা(সাজানো_তালিকা));
লেখ("");

// ২. বাইনারি সার্চ - দ্বিমিক অনুসন্ধান
লেখ("২. দ্বিমিক অনুসন্ধান (Binary Search):");

ফাংশন দ্বিমিক_অনুসন্ধান(তালিকা, লক্ষ্য) {
    ধরি বাম = ০;
    ধরি দক্ষিণ = দৈর্ঘ্য(তালিকা) - ১;

    যতক্ষণ (বাম <= দক্ষিণ) {
        ধরি মধ্য = বাম + ((দক্ষিণ - বাম) / ২);

        যদি (তালিকা[মধ্য] == লক্ষ্য) {
            ফেরত মধ্য;
        } নাহলে যদি (তালিকা[মধ্য] < লক্ষ্য) {
            বাম = মধ্য + ১;
        } নাহলে {
            দক্ষিণ = মধ্য - ১;
        }
    }

    ফেরত -১;
}

ধরি সাজানো = [১১, ১২, ২২, ২৫, ৩৪, ৪৫, ৬৪, ৮৮, ৯০];
ধরি খুঁজতে_হবে = ২৫;
ধরি অবস্থান = দ্বিমিক_অনুসন্ধান(সাজানো, খুঁজতে_হবে);

যদি (অবস্থান >= ০) {
    লেখ(লেখা(খুঁজতে_হবে) + " পাওয়া গেছে অবস্থানে: " + লেখা(অবস্থান));
} নাহলে {
    লেখ(লেখা(খুঁজতে_হবে) + " পাওয়া যায়নি");
}
লেখ("");

// ৩. ফিবোনাচি সংখ্যা - পুনরাবৃত্তি (Recursion)
লেখ("৩. ফিবোনাচি সংখ্যা (পুনরাবৃত্তি):");

ফাংশন ফিবোনাচি(সংখ্যা) {
    যদি (সংখ্যা <= ১) {
        ফেরত সংখ্যা;
    }
    ফেরত ফিবোনাচি(সংখ্যা - ১) + ফিবোনাচি(সংখ্যা - ২);
}

লেখ("প্রথম ১০টি ফিবোনাচি সংখ্যা:");
পর্যন্ত(ধরি গণনা = ০; গণনা < ১০; গণনা = গণনা + ১) {
    লেখ("ফিব(" + লেখা(গণনা) + ") = " + লেখা(ফিবোনাচি(গণনা)));
}
লেখ("");

// ৪. মৌলিক সংখ্যা পরীক্ষা এবং তালিকা
লেখ("৪. মৌলিক সংখ্যা:");

ফাংশন মৌলিক_কিনা(সংখ্যা) {
    যদি (সংখ্যা <= ১) {
        ফেরত মিথ্যা;
    }
    যদি (সংখ্যা <= ৩) {
        ফেরত সত্য;
    }
    যদি (সংখ্যা % ২ == ০ || সংখ্যা % ৩ == ০) {
        ফেরত মিথ্যা;
    }

    ধরি সূচক = ৫;
    যতক্ষণ (সূচক * সূচক <= সংখ্যা) {
        যদি (সংখ্যা % সূচক == ০ || সংখ্যা % (সূচক + ২) == ০) {
            ফেরত মিথ্যা;
        }
        সূচক = সূচক + ৬;
    }

    ফেরত সত্য;
}

লেখ("১ থেকে ৫০ পর্যন্ত মৌলিক সংখ্যা:");
ধরি মৌলিক_তালিকা = [];
পর্যন্ত(ধরি সংখ্যা = ২; সংখ্যা <= ৫০; সংখ্যা = সংখ্যা + ১) {
    যদি (মৌলিক_কিনা(সংখ্যা)) {
        ধরি পুরানো_দৈর্ঘ্য = দৈর্ঘ্য(মৌলিক_তালিকা);
        ধরি নতুন_তালিকা = নতুন(পুরানো_দৈর্ঘ্য + ১);

        পর্যন্ত(ধরি কপি = ০; কপি < পুরানো_দৈর্ঘ্য; কপি = কপি + ১) {
            নতুন_তালিকা[কপি] = মৌলিক_তালিকা[কপি];
        }
        নতুন_তালিকা[পুরানো_দৈর্ঘ্য] = সংখ্যা;
        মৌলিক_তালিকা = নতুন_তালিকা;
    }
}
লেখ(লেখা(মৌলিক_তালিকা));
লেখ("");

// ৫. সবচেয়ে বড় সাধারণ গুণনীয়ক (GCD) - ইউক্লিডিয়ান অ্যালগরিদম
লেখ("৫. সাধারণ গুণনীয়ক (GCD):");

ফাংশন সাধারণ_গুণনীয়ক(ক, খ) {
    যতক্ষণ (খ != ০) {
        ধরি অস্থায়ী = খ;
        খ = ক % খ;
        ক = অস্থায়ী;
    }
    ফেরত ক;
}

ধরি সংখ্যা_১ = ৪৮;
ধরি সংখ্যা_২ = ১৮;
ধরি গসাগু = সাধারণ_গুণনীয়ক(সংখ্যা_১, সংখ্যা_২);
লেখ("GCD(" + লেখা(সংখ্যা_১) + ", " + লেখা(সংখ্যা_২) + ") = " + লেখা(গসাগু));
লেখ("");

// ৬. প্যালিনড্রোম পরীক্ষা
লেখ("৬. প্যালিনড্রোম পরীক্ষা:");

ফাংশন প্যালিনড্রোম_কিনা(পাঠ্য) {
    ধরি দৈর্ঘ্য_মান = দৈর্ঘ্য(পাঠ্য);
    ধরি মধ্য = দৈর্ঘ্য_মান / ২;

    পর্যন্ত(ধরি সূচক = ০; সূচক < মধ্য; সূচক = সূচক + ১) {
        যদি (পাঠ্য[সূচক] != পাঠ্য[দৈর্ঘ্য_মান - সূচক - ১]) {
            ফেরত মিথ্যা;
        }
    }

    ফেরত সত্য;
}

ধরি শব্দ১ = "রাদার";
ধরি শব্দ২ = "বাংলা";

যদি (প্যালিনড্রোম_কিনা(শব্দ১)) {
    লেখ("'" + শব্দ১ + "' প্যালিনড্রোম");
} নাহলে {
    লেখ("'" + শব্দ১ + "' প্যালিনড্রোম নয়");
}

যদি (প্যালিনড্রোম_কিনা(শব্দ২)) {
    লেখ("'" + শব্দ২ + "' প্যালিনড্রোম");
} নাহলে {
    লেখ("'" + শব্দ২ + "' প্যালিনড্রোম নয়");
}

লেখ("");

// ৭. ম্যাট্রিক্স গুণ (Matrix Multiplication)
লেখ("৭. ম্যাট্রিক্স গুণ:");

ফাংশন ম্যাট্রিক্স_গুণ(ক, খ) {
    ধরি ক_সারি = দৈর্ঘ্য(ক);
    ধরি ক_কলাম = দৈর্ঘ্য(ক[০]);
    ধরি খ_কলাম = দৈর্ঘ্য(খ[০]);

    ধরি ফলাফল = [];
    পর্যন্ত(ধরি সারি = ০; সারি < ক_সারি; সারি = সারি + ১) {
        ধরি নতুন_সারি = [];
        পর্যন্ত(ধরি কলাম = ০; কলাম < খ_কলাম; কলাম = কলাম + ১) {
            ধরি যোগফল = ০;
            পর্যন্ত(ধরি গুণ_সূচক = ০; গুণ_সূচক < ক_কলাম; গুণ_সূচক = গুণ_সূচক + ১) {
                যোগফল = যোগফল + (ক[সারি][গুণ_সূচক] * খ[গুণ_সূচক][কলাম]);
            }
            ধরি পুরানো_দৈর্ঘ্য = দৈর্ঘ্য(নতুন_সারি);
            ধরি সম্প্রসারিত = নতুন(পুরানো_দৈর্ঘ্য + ১);
            পর্যন্ত(ধরি কপি = ০; কপি < পুরানো_দৈর্ঘ্য; কপি = কপি + ১) {
                সম্প্রসারিত[কপি] = নতুন_সারি[কপি];
            }
            সম্প্রসারিত[পুরানো_দৈর্ঘ্য] = যোগফল;
            নতুন_সারি = সম্প্রসারিত;
        }

        ধরি পুরানো_দৈর্ঘ্য = দৈর্ঘ্য(ফলাফল);
        ধরি সম্প্রসারিত = নতুন(পুরানো_দৈর্ঘ্য + ১);
        পর্যন্ত(ধরি কপি = ০; কপি < পুরানো_দৈর্ঘ্য; কপি = কপি + ১) {
            সম্প্রসারিত[কপি] = ফলাফল[কপি];
        }
        সম্প্রসারিত[পুরানো_দৈর্ঘ্য] = নতুন_সারি;
        ফলাফল = সম্প্রসারিত;
    }

    ফেরত ফলাফল;
}

ধরি ম্যাট্রিক্স_ক = [[১, ২], [৩, ৪]];
ধরি ম্যাট্রিক্স_খ = [[৫, ৬], [৭, ৮]];

লেখ("ম্যাট্রিক্স A:");
পর্যন্ত(ধরি সারি = ০; সারি < দৈর্ঘ্য(ম্যাট্রিক্স_ক); সারি = সারি + ১) {
    লেখ(লেখা(ম্যাট্রিক্স_ক[সারি]));
}

লেখ("ম্যাট্রিক্স B:");
পর্যন্ত(ধরি সারি = ০; সারি < দৈর্ঘ্য(ম্যাট্রিক্স_খ); সারি = সারি + ১) {
    লেখ(লেখা(ম্যাট্রিক্স_খ[সারি]));
}

ধরি গুণফল = ম্যাট্রিক্স_গুণ(ম্যাট্রিক্স_ক, ম্যাট্রিক্স_খ);
লেখ("গুণফল (A × B):");
পর্যন্ত(ধরি সারি = ০; সারি < দৈর্ঘ্য(গুণফল); সারি = সারি + ১) {
    লেখ(লেখা(গুণফল[সারি]));
}

লেখ("");

// ৮. তিন সংখ্যার যোগফল সমস্যা (3Sum Problem)
লেখ("৮. তিন সংখ্যার যোগফল:");

ফাংশন তিন_সংখ্যা_যোগফল(তালিকা, লক্ষ্য) {
    ধরি ফলাফল = [];
    ধরি দৈর্ঘ্য_মান = দৈর্ঘ্য(তালিকা);

    পর্যন্ত(ধরি প্রথম = ০; প্রথম < দৈর্ঘ্য_মান - ২; প্রথম = প্রথম + ১) {
        পর্যন্ত(ধরি দ্বিতীয় = প্রথম + ১; দ্বিতীয় < দৈর্ঘ্য_মান - ১; দ্বিতীয় = দ্বিতীয় + ১) {
            পর্যন্ত(ধরি তৃতীয় = দ্বিতীয় + ১; তৃতীয় < দৈর্ঘ্য_মান; তৃতীয় = তৃতীয় + ১) {
                ধরি যোগফল = তালিকা[প্রথম] + তালিকা[দ্বিতীয়] + তালিকা[তৃতীয়];

                যদি (যোগফল == লক্ষ্য) {
                    ধরি ত্রয়ী = [তালিকা[প্রথম], তালিকা[দ্বিতীয়], তালিকা[তৃতীয়]];
                    ধরি পুরানো_দৈর্ঘ্য = দৈর্ঘ্য(ফলাফল);
                    ধরি নতুন_ফলাফল = নতুন(পুরানো_দৈর্ঘ্য + ১);
                    পর্যন্ত(ধরি কপি = ০; কপি < পুরানো_দৈর্ঘ্য; কপি = কপি + ১) {
                        নতুন_ফলাফল[কপি] = ফলাফল[কপি];
                    }
                    নতুন_ফলাফল[পুরানো_দৈর্ঘ্য] = ত্রয়ী;
                    ফলাফল = নতুন_ফলাফল;
                }
            }
        }
    }

    ফেরত ফলাফল;
}

ধরি সংখ্যা_তালিকা = [১, ২, ৩, ৪, ৫, ৬];
ধরি লক্ষ্য_যোগফল = ১০;
