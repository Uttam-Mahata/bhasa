// ১. সাধারণ শ্রেণী - প্রাণী
লেখ("১. সাধারণ শ্রেণী সংজ্ঞা:");

শ্রেণী প্রাণী {
    সার্বজনীন নির্মাতা(নাম, বয়স) {
        এই.নাম = নাম;
        এই.বয়স = বয়স;
        লেখ("প্রাণী তৈরি হয়েছে: " + নাম);
    }

    সার্বজনীন পদ্ধতি তথ্য_দেখাও() {
        লেখ("নাম: " + এই.নাম + ", বয়স: " + লেখা(এই.বয়স));
        ফেরত এই.নাম;
    }

    সার্বজনীন পদ্ধতি শব্দ_করো() {
        লেখ("প্রাণী শব্দ করছে...");
    }
}

ধরি প্রাণী১ = নতুন প্রাণী("সাধারণ প্রাণী", 5);
প্রাণী১.তথ্য_দেখাও();
প্রাণী১.শব্দ_করো();
লেখ("");

// ২. উত্তরাধিকার - কুকুর প্রাণী থেকে প্রসারিত
লেখ("২. উত্তরাধিকার (প্রসারিত):");

শ্রেণী কুকুর প্রসারিত প্রাণী {
    সার্বজনীন নির্মাতা(নাম, বয়স, জাত) {
        উর্ধ্ব(নাম, বয়স);
        এই.জাত = জাত;
        লেখ("কুকুর তৈরি হয়েছে: " + নাম);
    }

    পুনর্সংজ্ঞা সার্বজনীন পদ্ধতি শব্দ_করো() {
        লেখ(এই.নাম + " ঘেউ ঘেউ করছে!");
    }

    সার্বজনীন পদ্ধতি জাত_দেখাও() {
        লেখ("জাত: " + এই.জাত);
    }
}

ধরি কুকুর১ = নতুন কুকুর("বাদল", 3, "জার্মান শেফার্ড");
কুকুর১.তথ্য_দেখাও();
কুকুর১.শব্দ_করো();
কুকুর১.জাত_দেখাও();
লেখ("");

// ৩. একাধিক স্তরের উত্তরাধিকার
লেখ("৩. একাধিক স্তরের উত্তরাধিকার:");

শ্রেণী পোষা_কুকুর প্রসারিত কুকুর {
    সার্বজনীন নির্মাতা(নাম, বয়স, জাত, মালিক) {
        উর্ধ্ব(নাম, বয়স, জাত);
        এই.মালিক = মালিক;
        লেখ("পোষা কুকুর তৈরি হয়েছে: " + নাম);
    }

    সার্বজনীন পদ্ধতি মালিক_দেখাও() {
        লেখ("মালিকের নাম: " + এই.মালিক);
    }

    পুনর্সংজ্ঞা সার্বজনীন পদ্ধতি শব্দ_করো() {
        লেখ(এই.নাম + " আনন্দে ঘেউ ঘেউ করছে!");
        লেখ("মালিক: " + এই.মালিক);
    }
}

ধরি পোষা১ = নতুন পোষা_কুকুর("টমি", 2, "ল্যাব্রাডর", "রহিম");
পোষা১.তথ্য_দেখাও();
পোষা১.শব্দ_করো();
পোষা১.জাত_দেখাও();
পোষা১.মালিক_দেখাও();
লেখ("");

// ৪. ব্যক্তিগত এবং সার্বজনীন সদস্য
লেখ("৪. ব্যক্তিগত এবং সার্বজনীন সদস্য:");

শ্রেণী ব্যাংক_হিসাব {
    ব্যক্তিগত ব্যালেন্স = 0;

    সার্বজনীন নির্মাতা(প্রাথমিক_ব্যালেন্স) {
        এই.ব্যালেন্স = প্রাথমিক_ব্যালেন্স;
        লেখ("হিসাব তৈরি হয়েছে। প্রাথমিক ব্যালেন্স: " + লেখা(প্রাথমিক_ব্যালেন্স));
    }

    সার্বজনীন পদ্ধতি জমা(পরিমাণ) {
        এই.ব্যালেন্স = এই.ব্যালেন্স + পরিমাণ;
        লেখ("জমা হয়েছে: " + লেখা(পরিমাণ));
    }

    সার্বজনীন পদ্ধতি উত্তোলন(পরিমাণ) {
        যদি (পরিমাণ <= এই.ব্যালেন্স) {
            এই.ব্যালেন্স = এই.ব্যালেন্স - পরিমাণ;
            লেখ("উত্তোলন হয়েছে: " + লেখা(পরিমাণ));
        } নাহলে {
            লেখ("অপর্যাপ্ত ব্যালেন্স!");
        }
    }

    সার্বজনীন পদ্ধতি ব্যালেন্স_দেখাও() {
        লেখ("বর্তমান ব্যালেন্স: " + লেখা(এই.ব্যালেন্স));
        ফেরত এই.ব্যালেন্স;
    }
}

ধরি হিসাব = নতুন ব্যাংক_হিসাব(10000);
হিসাব.জমা(5000);
হিসাব.উত্তোলন(3000);
হিসাব.ব্যালেন্স_দেখাও();
হিসাব.উত্তোলন(15000); // ব্যর্থ হবে
লেখ("");

// ৫. স্থির পদ্ধতি (Static methods simulation)
লেখ("৫. গণিত ক্লাস - স্থির পদ্ধতি:");

শ্রেণী গণিত {
    স্থির পদ্ধতি যোগ(ক, খ) {
        ফেরত ক + খ;
    }

    স্থির পদ্ধতি গুণ(ক, খ) {
        ফেরত ক * খ;
    }

    স্থির পদ্ধতি শক্তি(ভিত্তি, ঘাত) {
        ধরি ফলাফল = 1;
        পর্যন্ত(ধরি গণনা = 0; গণনা < ঘাত; গণনা = গণনা + 1) {
            ফলাফল = ফলাফল * ভিত্তি;
        }
        ফেরত ফলাফল;
    }
}

ধরি গণিত_উদাহরণ = নতুন গণিত();
লেখ("২ + ৩ = " + লেখা(গণিত_উদাহরণ.যোগ(2, 3)));
লেখ("৪ × ৫ = " + লেখা(গণিত_উদাহরণ.গুণ(4, 5)));
লেখ("২ ^ ৮ = " + লেখা(গণিত_উদাহরণ.শক্তি(2, 8)));
লেখ("");

// ৬. জটিল শ্রেণী - ছাত্র ব্যবস্থাপনা
লেখ("৬. জটিল শ্রেণী - ছাত্র ব্যবস্থাপনা:");

শ্রেণী ছাত্র {
    সার্বজনীন নির্মাতা(নাম, রোল, শ্রেণী) {
        এই.নাম = নাম;
        এই.রোল = রোল;
        এই.শ্রেণী = শ্রেণী;
        এই.নম্বর = {};
        লেখ("ছাত্র তৈরি: " + নাম + " (রোল: " + লেখা(রোল) + ")");
    }

    সার্বজনীন পদ্ধতি নম্বর_যোগ(বিষয়, নম্বর) {
        এই.নম্বর[বিষয়] = নম্বর;
        লেখ(বিষয় + " এ নম্বর যোগ হয়েছে: " + লেখা(নম্বর));
    }

    সার্বজনীন পদ্ধতি মোট_নম্বর() {
        ধরি মোট = 0;
        ধরি বিষয়গুলো = চাবি(এই.নম্বর);
        ধরি দৈর্ঘ্য = দৈর্ঘ্য(বিষয়গুলো);

        পর্যন্ত(ধরি সূচক = 0; সূচক < দৈর্ঘ্য; সূচক = সূচক + 1) {
            ধরি বিষয় = বিষয়গুলো[সূচক];
            মোট = মোট + এই.নম্বর[বিষয়];
        }

        ফেরত মোট;
    }

    সার্বজনীন পদ্ধতি গড়_নম্বর() {
        ধরি মোট = এই.মোট_নম্বর();
        ধরি বিষয়_সংখ্যা = দৈর্ঘ্য(চাবি(এই.নম্বর));
        যদি (বিষয়_সংখ্যা > 0) {
            ফেরত মোট / বিষয়_সংখ্যা;
        }
        ফেরত 0;
    }

    সার্বজনীন পদ্ধতি ফলাফল_দেখাও() {
        লেখ("===== ছাত্রের তথ্য =====");
        লেখ("নাম: " + এই.নাম);
        লেখ("রোল: " + লেখা(এই.রোল));
        লেখ("শ্রেণী: " + এই.শ্রেণী);
        লেখ("মোট নম্বর: " + লেখা(এই.মোট_নম্বর()));
        লেখ("গড় নম্বর: " + লেখা(এই.গড়_নম্বর()));
        লেখ("=========================");
    }
}

ধরি ছাত্র১ = নতুন ছাত্র("সুমন দাস", 101, "দশম");
ছাত্র১.নম্বর_যোগ("গণিত", 95);
ছাত্র১.নম্বর_যোগ("বিজ্ঞান", 88);
ছাত্র১.নম্বর_যোগ("ইংরেজি", 92);
ছাত্র১.নম্বর_যোগ("বাংলা", 97);
ছাত্র১.ফলাফল_দেখাও();
লেখ("");

// ৭. স্ট্রাক্ট এবং শ্রেণী একসাথে
লেখ("৭. স্ট্রাক্ট এবং শ্রেণী একসাথে:");

স্ট্রাক্ট ঠিকানা {
    রাস্তা,
    শহর,
    পিন_কোড
}

শ্রেণী ব্যক্তি {
    সার্বজনীন নির্মাতা(নাম, বয়স, ঠিকানা) {
        এই.নাম = নাম;
        এই.বয়স = বয়স;
        এই.ঠিকানা = ঠিকানা;
    }

    সার্বজনীন পদ্ধতি সম্পূর্ণ_তথ্য() {
        লেখ("নাম: " + এই.নাম);
        লেখ("বয়স: " + লেখা(এই.বয়স));
        লেখ("ঠিকানা:");
        লেখ("  রাস্তা: " + এই.ঠিকানা.রাস্তা);
        লেখ("  শহর: " + এই.ঠিকানা.শহর);
        লেখ("  পিন কোড: " + লেখা(এই.ঠিকানা.পিন_কোড));
    }
}

ধরি ঠিকানা১ = ঠিকানা { রাস্তা: "১২/এ পার্ক রোড", শহর: "ঢাকা", পিন_কোড: 1205 };
ধরি ব্যক্তি১ = নতুন ব্যক্তি("করিম আহমেদ", 35, ঠিকানা১);
ব্যক্তি১.সম্পূর্ণ_তথ্য();
লেখ("");

লেখ("=== সম্পূর্ণ OOP পরীক্ষা সফল! ===");
