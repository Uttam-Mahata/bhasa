// অপকোড_খুঁজো - অপকোড সংজ্ঞা খুঁজো
ধরি অপকোড_খুঁজো = ফাংশন(অপ) {
    ধরি কী = লেখা(অপ);
    যদি (চাবি_আছে(অপকোড_সংজ্ঞা, কী)) {
        ফেরত {"পাওয়া_গেছে": সত্য, "সংজ্ঞা": অপকোড_সংজ্ঞা[কী]};
    }
    ফেরত {"পাওয়া_গেছে": মিথ্যা, "সংজ্ঞা": নাল};
};

// নির্দেশ_তৈরি - বাইটকোড নির্দেশ তৈরি করো
ধরি নির্দেশ_তৈরি = ফাংশন(অপ, অপারেন্ড) {
    ধরি ফলাফল = অপকোড_খুঁজো(অপ);
    যদি (!ফলাফল["পাওয়া_গেছে"]) {
        ফেরত [];
    }
    
    ধরি সংজ্ঞা = ফলাফল["সংজ্ঞা"];
    ধরি প্রস্থ_তালিকা = সংজ্ঞা["অপারেন্ড_প্রস্থ"];
    
    // নির্দেশের মোট দৈর্ঘ্য গণনা করো
    ধরি নির্দেশ_দৈর্ঘ্য = ১;  // অপকোড এর জন্য ১ বাইট
    পর্যন্ত (ধরি i = ০; i < দৈর্ঘ্য(প্রস্থ_তালিকা); i = i + ১) {
        নির্দেশ_দৈর্ঘ্য = নির্দেশ_দৈর্ঘ্য + প্রস্থ_তালিকা[i];
    }
    
    // নির্দেশ অ্যারে তৈরি করো
    ধরি নির্দেশ = [];
    ধরি j = ০;
    পর্যন্ত (j = ০; j < নির্দেশ_দৈর্ঘ্য; j = j + ১) {
        নির্দেশ = যোগ(নির্দেশ, ০);
    }
    
    // অপকোড সেট করো
    নির্দেশ[০] = অপ;
    
    // অপারেন্ড এনকোড করো
    ধরি অফসেট = ১;
    পর্যন্ত (ধরি i = ০; i < দৈর্ঘ্য(প্রস্থ_তালিকা); i = i + ১) {
        ধরি প্রস্থ = প্রস্থ_তালিকা[i];
        ধরি মান = অপারেন্ড[i];
        
        যদি (প্রস্থ == ১) {
            // ১ বাইট অপারেন্ড
            নির্দেশ[অফসেট] = মান;
        } নাহলে যদি (প্রস্থ == ২) {
            // ২ বাইট অপারেন্ড (big-endian)
            নির্দেশ[অফসেট] = (মান >> ৮) & ২৫৫;
            নির্দেশ[অফসেট + ১] = মান & ২৫৫;
        }
        
        অফসেট = অফসেট + প্রস্থ;
    }
    
    ফেরত নির্দেশ;
};

// অপারেন্ড_পড়ো - নির্দেশ থেকে অপারেন্ড পড়ো
ধরি অপারেন্ড_পড়ো = ফাংশন(সংজ্ঞা, নির্দেশ) {
    ধরি প্রস্থ_তালিকা = সংজ্ঞা["অপারেন্ড_প্রস্থ"];
    ধরি অপারেন্ড = [];
    ধরি অফসেট = ০;
    
    পর্যন্ত (ধরি i = ০; i < দৈর্ঘ্য(প্রস্থ_তালিকা); i = i + ১) {
        ধরি প্রস্থ = প্রস্থ_তালিকা[i];
        
        যদি (প্রস্থ == ১) {
            // ১ বাইট অপারেন্ড
            অপারেন্ড = যোগ(অপারেন্ড, নির্দেশ[অফসেট]);
        } নাহলে যদি (প্রস্থ == ২) {
            // ২ বাইট অপারেন্ড (big-endian)
            ধরি উচ্চ_বাইট = নির্দেশ[অফসেট];
            ধরি নিম্ন_বাইট = নির্দেশ[অফসেট + ১];
            ধরি মান = (উচ্চ_বাইট << ৮) | নিম্ন_বাইট;
            অপারেন্ড = যোগ(অপারেন্ড, মান);
        }
        
        অফসেট = অফসেট + প্রস্থ;
    }
    
    ফেরত {"অপারেন্ড": অপারেন্ড, "বাইট_পড়া": অফসেট};
};

// নির্দেশ_থেকে_স্ট্রিং - নির্দেশকে স্ট্রিং-এ রূপান্তর করো (ডিবাগিংয়ের জন্য)
ধরি নির্দেশ_থেকে_স্ট্রিং = ফাংশন(নির্দেশ) {
    যদি (দৈর্ঘ্য(নির্দেশ) == ০) {
        ফেরত "";
    }
    
    ধরি আউটপুট = "";
    ধরি i = ০;
    
    যতক্ষণ (i < দৈর্ঘ্য(নির্দেশ)) {
        ধরি অপ = নির্দেশ[i];
        ধরি ফলাফল = অপকোড_খুঁজো(অপ);
        
        যদি (!ফলাফল["পাওয়া_গেছে"]) {
            আউটপুট = আউটপুট + "অজানা_অপকোড " + লেখা(অপ) + "\n";
            i = i + ১;
            চালিয়ে_যাও;
        }
        
        ধরি সংজ্ঞা = ফলাফল["সংজ্ঞা"];
        ধরি অপারেন্ড_ফলাফল = অপারেন্ড_পড়ো(সংজ্ঞা, অ্যারে_স্লাইস(নির্দেশ, i + ১, দৈর্ঘ্য(নির্দেশ)));
        
        আউটপুট = আউটপুট + প্যাড_করো(লেখা(i), ৪) + " " + সংজ্ঞা["নাম"];
        
        // অপারেন্ড প্রদর্শন করো
        ধরি অপারেন্ড = অপারেন্ড_ফলাফল["অপারেন্ড"];
        পর্যন্ত (ধরি j = ০; j < দৈর্ঘ্য(অপারেন্ড); j = j + ১) {
            আউটপুট = আউটপুট + " " + লেখা(অপারেন্ড[j]);
        }
        
        আউটপুট = আউটপুট + "\n";
        i = i + ১ + অপারেন্ড_ফলাফল["বাইট_পড়া"];
    }
    
    ফেরত আউটপুট;
};

// নির্দেশ_জোড়া_লাগাও - দুটি নির্দেশ অ্যারে জোড়া লাগাও
ধরি নির্দেশ_জোড়া_লাগাও = ফাংশন(নির্দেশ১, নির্দেশ২) {
    ধরি ফলাফল = [];
    
    পর্যন্ত (ধরি i = ০; i < দৈর্ঘ্য(নির্দেশ১); i = i + ১) {
        ফলাফল = যোগ(ফলাফল, নির্দেশ১[i]);
    }
    
    পর্যন্ত (ধরি i = ০; i < দৈর্ঘ্য(নির্দেশ২); i = i + ১) {
        ফলাফল = যোগ(ফলাফল, নির্দেশ২[i]);
    }
    
    ফেরত ফলাফল;
};

// নির্দেশ_দৈর্ঘ্য - নির্দেশের দৈর্ঘ্য পাও
ধরি নির্দেশ_দৈর্ঘ্য = ফাংশন(নির্দেশ) {
    ফেরত দৈর্ঘ্য(নির্দেশ);
};

// অ্যারে_স্লাইস - অ্যারের অংশ নাও (helper function)
ধরি অ্যারে_স্লাইস = ফাংশন(অ্যারে, শুরু, শেষ) {
    ধরি ফলাফল = [];
    পর্যন্ত (ধরি i = শুরু; i < শেষ; i = i + ১) {
        ফলাফল = যোগ(ফলাফল, অ্যারে[i]);
    }
    ফেরত ফলাফল;
};

// প্যাড_করো - স্ট্রিং প্যাড করো (helper function)
ধরি প্যাড_করো = ফাংশন(স্ট্রিং, দৈর্ঘ্য) {
    যতক্ষণ (দৈর্ঘ্য(স্ট্রিং) < দৈর্ঘ্য) {
        স্ট্রিং = "০" + স্ট্রিং;
    }
    ফেরত স্ট্রিং;
};

// =============== নির্দেশ তৈরির সহায়ক ফাংশন ===============

// ধ্রুবক_নির্দেশ - ধ্রুবক লোড নির্দেশ তৈরি করো
